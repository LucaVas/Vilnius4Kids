{
  "name": "@vilnius4kids/server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts && npm run sentry:sourcemaps",
    "coverage": "DB_TYPE=pg-mem vitest run --coverage",
    "dev": "tsx watch src",
    "dev:mem": "DB_TYPE=pg-mem tsx watch src",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "start": "node ./dist/index.js",
    "test": "npm run test:mem",
    "test:mem": "DB_TYPE=pg-mem vitest",
    "test:db": "vitest",
    "typecheck": "tsc --noEmit",
    "typeorm": "NODE_OPTIONS='-r tsconfig-paths/register' typeorm-ts-node-commonjs",
    "migration:create": "node src/database/cli 'migration:create src/database/migrations/$0'",
    "migration:generate": "node src/database/cli 'migration:generate src/database/migrations/$0 -d src/database/cli/dataSource.ts'",
    "migration:revert": "npm run typeorm migration:revert -- -d src/database/cli/dataSource.ts",
    "migration:run": "tsx ../node_modules/typeorm/cli.js migration:run -d ./src/database/cli/dataSource.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/database/cli/dataSource.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org vilnius4kids --project vilnius4kids_server ./dist && sentry-cli sourcemaps upload --org vilnius4kids --project vilnius4kids_server ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.8.2",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "pg-mem": "^2.7.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "tsup": "^7.2.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@sentry/cli": "^2.26.0",
    "@sentry/node": "^7.98.0",
    "@sentry/profiling-node": "^1.3.5",
    "@trpc/server": "^10.41.0",
    "@types/chance": "^1.1.6",
    "@types/nodemailer": "^6.4.14",
    "@types/pino": "^7.0.5",
    "@types/pino-pretty": "^5.0.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^8.0.0",
    "chance": "^1.1.11",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nodemailer": "^6.9.8",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "readline-sync": "^1.4.10",
    "superjson": "^2.2.1",
    "trpc-panel": "^1.3.4",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  },
  "tsup": {
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "external": [
      "@mikro-orm",
      "mysql",
      "pg-promise",
      "slonik",
      "sqlite3",
      "kysely"
    ],
    "noExternal": [
      "superjson"
    ]
  }
}